<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" style="-fx-background-color: teal;"
  maxHeight="-Infinity" maxWidth="-Infinity"
  minHeight="-Infinity" minWidth="-Infinity" prefHeight="628.0" prefWidth="592.0"
  xmlns="http://javafx.com/javafx/8.0.171" fx:controller="createaccount.CreateAccountController"
  fx:id="root">
  <children>
    <Label layoutX="23.0" layoutY="100" prefHeight="26.0" prefWidth="152.0" text="UserName:">
      <font>
        <Font name="System Bold" size="15.0"/>
      </font>
    </Label>
    <Label layoutX="23.0" layoutY="150" prefHeight="26.0" prefWidth="127.0" text="Password:">
      <font>
        <Font name="System Bold" size="15.0"/>
      </font>
    </Label>
    <PasswordField layoutX="175.0" layoutY="138.0" prefHeight="26.0" prefWidth="152.0"
      fx:id="password"/>
    <TextField layoutX="175.0" layoutY="99.0" prefHeight="26.0" prefWidth="152.0" fx:id="username"/>
    <Label graphicTextGap="7.0" layoutX="23.0" layoutY="45.0" prefHeight="47.0" prefWidth="300.0"
      text="Create your Car Share Account" wrapText="true">
      <font>
        <Font name="Arial Bold" size="20.0"/>
      </font>
    </Label>
    <Label layoutX="23.0" layoutY="200" prefHeight="26.0" prefWidth="117.0"
      text="Confirm Password:">
      <font>
        <Font name="System Bold" size="13.0"/>
      </font>
    </Label>
    <PasswordField layoutX="175.0" layoutY="187.0" prefHeight="26.0" prefWidth="152.0"
      fx:id="confirmPassword"/>
    <Label layoutX="23.0" layoutY="257.0" prefHeight="26.0" prefWidth="110.0" text="Email:">
      <font>
        <Font name="System Bold" size="15.0"/>
      </font>
    </Label>
    <TextField layoutX="175.0" layoutY="255.0" prefHeight="26.0" prefWidth="201.0" fx:id="email"/>
    <Label layoutX="23.0" layoutY="314.0" prefHeight="18.0" prefWidth="110.0" text="Phone Number:">
      <font>
        <Font name="System Bold" size="14.0"/>
      </font>
    </Label>
    <TextField layoutX="175.0" layoutY="310.0" prefHeight="26.0" prefWidth="201.0"
      fx:id="phoneNumber"/>
    <Button layoutX="175.0" layoutY="369.0" mnemonicParsing="false"
      onAction="#onCreateYourAccountPressed" text="Create Your Account"/>
    <ToolBar prefHeight="45.0" prefWidth="592.0">
      <items>
        <Button mnemonicParsing="false" onAction="#onBackToHomepagePressed"
          text="back to homepage"/>
      </items>
    </ToolBar>
  </children>
</AnchorPane>
